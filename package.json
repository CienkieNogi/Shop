{
  "name": "app",
  "version": "1.0.0",
  "main": "bootstrap.ts",
  "author": "Donny Roufs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --no-notify src/bootstrap.ts",
    "start": "nodemon --delay 500ms src/bootstrap.ts",
    "test": "jest",
    "build": "tsc",
    "test:watch": "jest --watch --verbose",
    "lint": "eslint . --ext .ts",
    "prepare": "husky install"
  },
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.31.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "reflect-metadata": "^0.1.13",
    "ts-guard-clause": "^0.0.7",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@prisma/client": "^4.2.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.3.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": ">=12",
    "nodemon": "^2.0.20",
    "prettier": "^2.5.0",
    "prisma": "^4.2.1",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn eslint",
      "yarn prettier --write",
      "yarn test --watchAll=false --findRelatedTests --bail"
    ]
  }
}
